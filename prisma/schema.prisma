generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(cuid())
  email          String   @unique
  name           String?
  image          String?
  apiKey         String
  credits        Int      @default(4)
  rechargeCount  Int      @default(0)
  createdAt      DateTime @default(now())
}
